#define IncSubdir nx-X11

all::

DEFINES = $(FAILDEFINES)

# Since xtrans source is #include'd into software that uses it, we install
# the .c files as if they were headers.
HEADERS = \
	Xtrans.h \
	Xtrans.c \
	Xtransint.h \
	Xtranslcl.c \
	Xtransos2.c \
	Xtranssock.c \
	Xtransutil.c \
	transport.c

DependTarget()
BuildIncludes($(HEADERS),IncSubdir/Xtrans,../..)
