      ProgramTargetName(sphere) \
      ProgramTargetName(star) \
      ProgramTargetName(stencil) \
      ProgramTargetName(stretch) \
      ProgramTargetName(texture) \
      ProgramTargetName(tri) \
      ProgramTargetName(wave)

